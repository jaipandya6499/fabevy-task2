*{
margin:0;
padding:0;
box-sizing:border-box;
}
body{
	/*font-family: 'Noto Sans', sans-serif;*/
    font-family: 'Poppins', sans-serif;
	background-color:#f5f5f5;
}
.container{
	width:1140px;
	margin: 0 auto;
	background-color:white;
	padding:20px 30px;
}

/*-------- header-info style start  --------------*/
.header-info .name{
	border-bottom:1.5px solid #ccc;
    padding-bottom: 10px;
	margin-bottom:10px;
}

.header-info{
	display:flex;
	align-items:center;
}
.header-info .header-details{
	flex-grow:1;
}

.myimage img{
	width:110px;
}
.header-details{
	width:84%;
	padding-left:30px;
}
.header-info .navbar ul{
	list-style:none;
}
.header-info .navbar li{
	display:inline-block;
	margin:0 30px;
	color:#ccc;
} 
.navbar ul li a{
	color:#ccc;
	text-decoration:none;
	display:block;
} 
.navbar ul li a.active{
	color:#000;
}
.navbar ul li a:hover{
	color:#000;
}
    /*-------- header-info style end  --------------*/
	
	/*-------- profile style start  --------------*/
	
.profile{
	
	background-color:#000;
	padding: 50px 10px;
	text-align:center;
	margin-top:30px;
}
.profile-content h1{
	color:#fff;
	font-size:35px;
	
}
.profile-content h3{
	color:#999;
	font-size:17px;
	margin-bottom:10px;
}
.profile-content p{
	color:#f5f5f5;
	margin-top:10px;
}
.profile-content a{
	text-decoration:none;
	font-size:15px;
	padding: 10px 15px;
	border:1px solid white;
	display:inline-block;
	color:#fff;
	margin-top:15px;
}
.line{
	border-bottom:1.5px solid #ccc;
	margin-top:50px;
}

    /*-------- profile style end  --------------*/
	
	/*-------- Skills Style Start  --------------*/

.skills{
	margin:30px 0 0 0;
	border-bottom:1.5px solid #ccc;
	padding-bottom:60px;
}
.skills h1{
	font-size:30px;
}
.skills-attach{
	display:flex;
	margin:20px 0 0 0;
}
.skills-attach img{
	width:30px;
}
.skills-attach p{
	font-size:12px;
	font-family:cursive;
	margin:0 0 0 25px;
}
.skills-nav ul{
	display:flex;
	margin-top:40px;
}
.skills-nav ul li{
	list-style:none;
	padding-right:80px;
}
 
 
        /*-------- Skills Style End  --------------*/
		
		 /*-------- Education Style Start  --------------*/
		 
.education{
	margin:20px 0 0 0;
	border-bottom:1.5px solid #ccc;
	padding-bottom: 40px;
}
.education h1{
	font-size:30px;
}
.edu-para{
	margin-top:23px;
	display:flex;
}
.edu-para i{
	font-size:35px;
}
.edu-para p{
	font-size:12px;
	font-family:cursive;
	margin:0 0 0 18px;
}


                  /*---Percentage Style----*/
       
.education .para-1{
	margin:40px 0 0 0;
 }
 .para-1{
	display:flex;
	border-left:3px solid #ccc;
}
     
.para-1 .p-0 {
	color:#B6B6B4;
}
.para-1 h1{
	font-size:20px;
}
.para-1 .p-2 {
	color:#52595D;
}
.para-details{
	margin-left:13px;
}
.para-1 .percentage{
	border:3px solid #ccc;
	padding:35px;
	text-align:center;
    margin-left:450px;
	background-color:#ccc;
}
		/*-------- Education Style End  --------------*/
		
     /*-------- Projects Style Start  --------------*/
	 
.projects{
	margin:20px 0 0 0;
	border-bottom:1.5px solid #ccc;
	padding-bottom: 40px;
}
.projects h1{
	font-size:30px;
}
.pro{
	margin-top:23px;
	display:flex;
}
.pro i{
	font-size:35px;
}
.pro p{
	font-size:12px;
	font-family:cursive;
	margin:0 0 0 18px;
}
                 /*--projects menu style--*/
.pro-1{
	margin-top:30px;
	padding-left:15px;
	display:flex;
	border-left:3px solid #ccc;
}	
.pro-details .p0{
	color:#979A9A ;
	padding-bottom:3px; 
}
.pro-details .p1{
	color:#979A9A ;
	font-size:12px;
	padding-top:3px;
}
.pro-details .p2{
	color:#979A9A;
}
.pro-details .p3{
	color:#979A9A;
}
	 
	 /*-------- Projects Style End  --------------*/
  
         /*----------- Contact Style Start  ------------------*/

.contact{
	margin-top:27px;
}
.contact h1{
	font-size:30px;
}	
.contact p{
	font-size:12px;
	color:#979A9A;
} 
.contact-media{
	margin-top:20px;
}
.contact-media i{
	font-size:34px;
	margin:0 0px;
	padding-right:35px;

}
.copyrights p{
	margin-top:20px;
	color:#5D6D7E;
	font-family:arial;
}
 	       /*---------- Contact Style End  ------------------*/
 
 
 
 